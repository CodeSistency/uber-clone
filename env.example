# Environment Configuration Template
# Copy this file to .env.development or .env.production and fill with actual values

# Server Configuration
EXPO_PUBLIC_SERVER_URL=http://localhost:3000
EXPO_PUBLIC_WS_URL=ws://localhost:3001

# Firebase Configuration
EXPO_PUBLIC_FIREBASE_API_KEY=your_firebase_api_key_here
EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
EXPO_PUBLIC_FIREBASE_PROJECT_ID=your_project_id

# Payment Configuration (Stripe)
EXPO_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key

# Maps Configuration (Google Maps)
EXPO_PUBLIC_GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# App Configuration
EXPO_PUBLIC_APP_ENV=development
EXPO_PUBLIC_ENABLE_DEBUG=true
EXPO_PUBLIC_ENABLE_ANALYTICS=false

# Optional: Additional configurations
EXPO_PUBLIC_SENTRY_DSN=your_sentry_dsn_here
EXPO_PUBLIC_MIXPANEL_TOKEN=your_mixpanel_token_here
