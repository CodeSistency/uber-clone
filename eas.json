{
  "cli": {
    "version": ">= 16.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "gradleCommand": ":app:assembleDebug"
      },
      "prebuildCommand": "npm run prebuild:dev"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "prebuildCommand": "npm run prebuild:dev"
    },
    "production": {
      "android": {
        "buildType": "app-bundle"
      },
      "prebuildCommand": "npm run prebuild:prod"
    }
  },
  "submit": {
    "production": {}
  }
}
